{
  "cli": { 
    "version": ">= 10.0.0" 
  },
  "build": {
    "development": { 
      "developmentClient": true, 
      "ios": {
        "simulator": true
      }, 
      "android": {
        "buildType": "apk"
      }
    },
    "preview": { 
      "ios": {
        "simulator": false,
        "distribution": "internal"
      }, 
      "android": {
        "buildType": "apk"
      }
    },
    "production": { 
      "ios": {
        "simulator": false,
        "distribution": "store"
      }, 
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": { 
        "ascAppId": "6751903645"
      },
      "android": { 
        "track": "internal", 
        "serviceAccountKeyPath": "./play-sa.json" 
      }
    }
  }
}